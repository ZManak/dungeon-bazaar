{
  "name": "dungeon",
  "version": "1.0.0",
  "description": "A fullstack magic store",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
     "server": "nodemon server.js",
     "client": "npm start --prefix client",
     "clientinstall": "npm install --prefix client",
     "dev": "concurrently \"npm run server\" \"npm run client\"",
     "deploy":"concurrently \"npm run server\" \"npm run client\"",
     "render-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "seeds": "set NODE_ENV=development && (node ./seeders/item_seed.js && node ./seeders/maker_seed.js)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZManak/dungeon-bazaar.git"
  },
  "keywords": [
    "store",
    "backend",
    "frontend",
    "fullstack",
    "react",
    "express"
  ],
  "author": "ZManak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZManak/dungeon-bazaar/issues"
  },
  "homepage": "https://github.com/ZManak/dungeon-bazaar#readme",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "babel-register": "^6.26.0",
    "nodemon": "^2.0.21",
    "sequelize-cli": "^6.6.0"
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "node": "^19.6.1",
    "pg": "^8.9.0",
    "sequelize": "^6.29.0",
    "uuid": "^9.0.0"
  }
}
